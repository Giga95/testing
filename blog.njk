---
title: Duos Asinos Consulting - Blog
layout: 'base.njk'
---

    <main>
      <section class="half-heading">
        <div class="page-heading blog">
          <h2>Blog</h2>
        </div>
      </section>
      <section class="blog-section">
        <div class="container">
          <div class="category-filter">
            {# <div class="category-head">
              <div class="category-lists">
                <ul>
                  <div class="category-title active-category" id="all">
                    <li>All</li>
                  </div>
                  <div class="category-title" id="career-category">
                    <li>Career</li>
                  </div>
                  <div class="category-title" id="news-category">
                    <li>News</li>
                  </div>
                  <div class="category-title" id="study-category">
                    <li>Study</li>
                  </div>
                  <div class="category-title" id="tech-category">
                    <li>Tech</li>
                  </div>
                  <div class="category-title" id="other-category">
                    <li>Other</li>
                  </div>
                </ul>
              </div>
              <div class="category-search">
                <form class="search">
                  <input
                    class="search-box"
                    type="text"
                    placeholder="Search..."
                  />
                  <button type="submit" class="submit-search">
                    <img src="/img/icons/search.png" alt="search" />
                  </button>
                </form>
              </div>
            </div> #}
            <div class="posts">

              {%- for post in collections.post | reverse -%}

              <div class="all career-category">
                <div class="post-box">
                  <a href="{{post.url}}" target="_blank">
                    <div class="post-img">
                      <img src="{{post.data.image}}" alt="{{post.data.imageAlt}}" />
                    </div>
                  </a>
                  {# img #}
                  <div class="post-content">
                    <a href="{{post.url}}" target="_blank">
                      <h2>{{post.data.title}}</h2>
                    </a>
                    
                      <p>{{post.data.description}}</p>
                    {# title, desc #}

                  </div>

                  <div class="blog-info">
                    <p>{{ post.data.tag }}</p>
                    <a
                      href="{{post.url}}"
                      target="_blank"
                      type="button"
                      class="read-btn"
                      >Read more</a>
                    {# btn #}
                  </div>
                </div>
              </div>

              {%- endfor -%}              


              
            </div>
          </div>
        </div>
      </section>
    </main>
<script src="/js/category-filter.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
